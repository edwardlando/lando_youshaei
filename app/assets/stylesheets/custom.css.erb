@font-face {
    font-family: 'poiret_oneregular';
    src: url('<%= asset_path('poiretone-regular-webfont.eot') %>');
    src: url('<%= asset_path('poiretone-regular-webfont.eot?#iefix') %>') format('embedded-opentype'),
         url('<%= asset_path('poiretone-regular-webfont.woff') %>') format('woff'),
         url('<%= asset_path('poiretone-regular-webfont.ttf') %>') format('truetype'),
         url('<%= asset_path('poiretone-regular-webfont.svg#poiret_oneregular') %>') format('svg');
    font-weight: normal;
    font-style: normal;

}

